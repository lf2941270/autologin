{
    "manifest_version": 2,
    "name": "autoLogin",
    "version": "1.0",
    "description": "autoLogin Capstone",
    "browser_action": {
        "default_icon": {
            "19": "images/icon19_0.png",
            "38": "images/icon38_0.png"
        },
        "default_title": "autoLogin"
    },
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    }
}